public class SimpleCalulator{

    //declare data members
    //input
    private double num1;
    private double num2;
    //output
    private double result;
    
    public void substract(){

	result = num1 - num2;
    } 

    public void displayResult(){

	System.out.println("first number = " + num1 + ";second number = "+ num2 + "result = " +result);
    }

    
    
}
